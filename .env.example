# Example environment configuration for Agent AI App Factory
# Copy to .env and populate with your credentials

# Core API tokens
OPENAI_API_KEY=your-openai-api-key
GITHUB_TOKEN=your-github-token
VERCEL_TOKEN=your-vercel-token

# Optional Vercel team slug
VERCEL_TEAM=your-vercel-team

# Supabase configuration
SUPABASE_URL=your-supabase-url
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Additional data sources
ENVATO_TOKEN=your-envato-token
REDDIT_USER_AGENT=your-reddit-user-agent

# Marketplace integrations
FLIPPA_API_KEY=your-flippa-api-key
FLIPPA_API_URL=https://api.flippa.com/v3/listings
INTERNAL_MARKETPLACE_URL=your-marketplace-url

# Backend server port
PORT=4000

# Frontend API base URL
NEXT_PUBLIC_API_BASE_URL=
